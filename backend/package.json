{
      "name": "finsim-backend",
      "version": "1.0.0",
      "description": "FinSim Backend Service",
      "main": "dist/server.js",
      "scripts": {
            "build": "tsc",
            "start": "node dist/server.js",
            "dev": "tsc-watch --onSuccess 'node dist/server.js'",
            "db:migrate": "npx sequelize-cli db:migrate",
            "db:seed:all": "npx sequelize-cli db:seed:all",
            "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
            "lint": "eslint . --ext .ts"
      },
      "keywords": [],
      "author": "",
      "license": "ISC",
      "dependencies": {
            "cors": "^2.8.5",
            "dotenv": "^16.0.3",
            "express": "^4.18.2",
            "firebase-admin": "^11.11.0",
            "joi": "^17.13.3",
            "jsonwebtoken": "^9.0.2",
            "pg": "^8.11.3",
            "pg-hstore": "^2.3.4",
            "sequelize": "^6.35.1"
      },
      "devDependencies": {
            "@types/cors": "^2.8.17",
            "@types/express": "^4.17.17",
            "@types/node": "^20.8.9",
            "@types/sequelize": "^4.28.19",
            "@types/validator": "^13.11.7",
            "@typescript-eslint/eslint-plugin": "^6.4.0",
            "@typescript-eslint/parser": "^6.4.0",
            "eslint": "^8.0.1",
            "sequelize-cli": "^6.6.2",
            "tsc-watch": "^6.0.4",
            "typescript": "^5.2.2"
      }
